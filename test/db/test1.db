test1	{
	id		varchar(40);
	class	varchar(20);
	code	int;
};

primary	{
	code;
};

path	id	{
	DBSELECT	{
		DECLARE test1_id_csr CURSOR FOR
		SELECT	*
			WHERE
				code = :code
		;
	};
};

path	id2	{
	DBSELECT	{
		DECLARE test1_id2_csr CURSOR FOR
#		SELECT	id
		SELECT	*
			FROM test1
			WHERE
				code <= :code
		;
	};
};

path	id3	{
	DBSELECT	{
		SELECT	*
			FROM test1
			WHERE
				code <= :code
			LIMIT
				: $limit
		;
	};
};

